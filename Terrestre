public class Terrestre extends Animal {
    protected String Habitat;

    public Terrestre(String Nombre, int Edad, String Tipo_de_Alimentación, 
        String Respiración, double Peso, String Tipo_de_Animal, String Habitat) {
        super(Nombre, Edad, Tipo_de_Alimentación, Respiración, Peso, Tipo_de_Animal);
        this.Habitat = Habitat;
    }

    public void Cazar() {
        System.out.println(Nombre + " Está cazando en " + Habitat);
    }

    @Override
    public void Emitir_Sonido() {
        System.out.println(Nombre + " Ruge.");
    }

    @Override
    public void Moverse() {
        System.out.println(Nombre + " Camina.");
    }
}
